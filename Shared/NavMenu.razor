	void SetTab(TabsModel item) => (UiUpdate.SelectedTab, UiUpdate.SelectedTabIndex, collapseNavMenu) =
		(CVModel.CV.IsValid() && !CRUD.IsInEditState) || (UiUpdate.SelectedTab is Tabs.Personal && item.Tab is Tabs.HomePage)
		? (item!.Tab, CVModel.CV.ListOfTabs.Where(x => x.IsSelected).ToList().IndexOf(item), true) : (UiUpdate.SelectedTab, UiUpdate.SelectedTabIndex, true);
